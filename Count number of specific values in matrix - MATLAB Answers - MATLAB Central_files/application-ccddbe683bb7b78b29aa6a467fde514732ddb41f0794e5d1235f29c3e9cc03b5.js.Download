      toggle_contextual_error(this.$input);
    }
  }
};

const productMulticompleteConfiguration = {
  openOnFocus: false,
  highlight: false,
  selectOnTab: true,
  closeAfterSelect: true,
  copyClassesToDropdown: false, 
  render: multicompleteRenderer,        
  delimiter: localized_delimiter(renderer_locale),               
